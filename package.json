{
  "name": "nestjs-telegraf",
  "version": "2.4.0",
  "description": "Telegraf module for NestJS",
  "keywords": [
    "nest",
    "nestjs",
    "nestjs-module",
    "telegraf",
    "telegram",
    "telegram bot",
    "telegram bot api",
    "bot",
    "bot api",
    "bot framework"
  ],
  "homepage": "https://nestjs-telegraf.vercel.app/",
  "bugs": {
    "url": "https://github.com/webjohny/nestjs-telegraf/issues"
  },
  "repository": "git@github.com:webjohny/nestjs-telegraf.git",
  "license": "MIT",
  "author": "Alexander Bukhalo <a@bukhalo.com>",
  "contributors": [
    {
      "name": "Arthur Asimov",
      "email": "arthur.asimov.z0@gmail.com"
    }
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "",
    "typedoc:build": "typedoc --tsconfig ./tsconfig.typedoc.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-angular": "16.2.4",
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@types/lodash": "4.14.182",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "reflect-metadata": "0.1.13",
    "telegraf": "4.8.0",
    "typedoc": "0.22.15",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.7.0 || ^7.0.0 || ^8.0.0",
    "@nestjs/core": "^6.7.0 || ^7.0.0 || ^8.0.0",
    "reflect-metadata": "^0.1.13",
    "telegraf": "^4.0.0",
    "typescript": "^4.1.2"
  }
}
